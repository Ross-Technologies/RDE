{
	"name": "Windows 9x",
	"description": "A Windows 9x theme.",
	"overrides": ["wallpaper","sounds","wm","startup","gtk"],
	"images": {
		"wallpaper": "wallpaper.png"
	},
	"sounds": {
		"startup": "startup.mp3"
	},
	"startup": [
		"fbpanel --profile win9x"
	],
	"gtkTheme": "win9x",
	"wm": "openbox --config-file ~/.local/share/rde/themes/win9x/config/openbox/rc.xml"
}
